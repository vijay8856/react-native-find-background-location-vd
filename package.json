{
  "name": "react-native-find-background-location-vd",
  "version": "1.0.0",
  "description": "Android foreground service based background location tracking for React Native",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
"types": "lib/typescript/commonjs/index.d.ts",

  "private": false,

"files": [
  "android/src",
  "android/build.gradle",
  "android/gradle.properties",
  "ios",
  "lib",
  "react-native.config.js",
  "*.podspec",
  "README.md",
  "LICENSE"
],
  "scripts": {
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "build": "bob build",
    "prepare": "bob build",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest"
  },

  "keywords": [
    "react-native",
    "react-native-library",
    "background-location",
    "foreground-service",
    "android"
  ],

  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijay8856/react-native-find-background-location-vd.git"
  },

  "author": "Vijay Dhakad <vijaydhakad8871@gmail.com> (https://github.com/vijay8856)",
  "license": "MIT",

  "bugs": {
    "url": "https://github.com/vijay8856/react-native-find-background-location-vd/issues"
  },

  "homepage": "https://github.com/vijay8856/react-native-find-background-location-vd#readme",

  "publishConfig": {
    "access": "public"
  },

  "peerDependencies": {
    "react": ">=17",
    "react-native": ">=0.71"
  },

  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.40.13",
    "typescript": "^5.9.2"
  },

  "workspaces": ["example"],

  "packageManager": "yarn@4.11.0",

  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      ["commonjs"],
      ["module", { "esm": true }],
      ["typescript", { "project": "tsconfig.build.json" }]
    ]
  },

  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
}
